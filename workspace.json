{
  "version": 1,
  "projects": {
    "booking": {
      "root": "apps/booking",
      "sourceRoot": "apps/booking/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/web:build",
          "options": {
            "outputPath": "dist/apps/booking",
            "index": "apps/booking/src/index.html",
            "main": "apps/booking/src/main.tsx",
            "polyfills": "apps/booking/src/polyfills.ts",
            "tsConfig": "apps/booking/tsconfig.app.json",
            "assets": [
              "apps/booking/src/favicon.ico",
              "apps/booking/src/assets"
            ],
            "styles": [],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/babel"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/booking/src/environments/environment.ts",
                  "with": "apps/booking/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "booking:build",
            "proxyConfig": "apps/booking/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "buildTarget": "booking:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/booking/.eslintrc",
            "tsConfig": [
              "apps/booking/tsconfig.app.json",
              "apps/booking/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!apps/booking/**"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/booking/jest.config.js",
            "tsConfig": "apps/booking/tsconfig.spec.json"
          }
        }
      }
    },
    "booking-e2e": {
      "root": "apps/booking-e2e",
      "sourceRoot": "apps/booking-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/booking-e2e/cypress.json",
            "tsConfig": "apps/booking-e2e/tsconfig.e2e.json",
            "devServerTarget": "booking:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "booking:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/booking-e2e/.eslintrc",
            "tsConfig": [
              "apps/booking-e2e/tsconfig.e2e.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!apps/booking-e2e/**"
            ]
          }
        }
      }
    },
    "utilities": {
      "root": "libs/utilities",
      "sourceRoot": "libs/utilities/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "libs/utilities/.eslintrc",
            "tsConfig": [
              "libs/utilities/tsconfig.lib.json",
              "libs/utilities/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!libs/utilities/**"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/utilities/jest.config.js",
            "tsConfig": "libs/utilities/tsconfig.spec.json"
          }
        }
      }
    },
    "firebase": {
      "root": "libs/firebase",
      "sourceRoot": "libs/firebase/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "libs/firebase/.eslintrc",
            "tsConfig": [
              "libs/firebase/tsconfig.lib.json",
              "libs/firebase/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!libs/firebase/**"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/firebase/jest.config.js",
            "tsConfig": "libs/firebase/tsconfig.spec.json"
          }
        }
      }
    },
    "booking-api": {
      "root": "apps/booking-api",
      "sourceRoot": "apps/booking-api/src",
      "projectType": "application",
      "prefix": "booking-api",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/booking-api",
            "main": "apps/booking-api/src/main.ts",
            "tsConfig": "apps/booking-api/tsconfig.app.json",
            "assets": [
              "apps/booking-api/src/assets"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/booking-api/src/environments/environment.ts",
                  "with": "apps/booking-api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "booking-api:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/booking-api/.eslintrc",
            "tsConfig": [
              "apps/booking-api/tsconfig.app.json",
              "apps/booking-api/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!apps/booking-api/**"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/booking-api/jest.config.js",
            "tsConfig": "apps/booking-api/tsconfig.spec.json"
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "schematics": {
    "@nrwl/workspace": {
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "eslint"
      }
    },
    "@nrwl/react": {
      "application": {
        "style": "styled-components",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "styled-components"
      },
      "library": {
        "style": "styled-components",
        "linter": "eslint"
      }
    },
    "@nrwl/next": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/web": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/nest": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/express": {
      "application": {
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "booking"
}
